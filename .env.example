# Environment Variables

# Ticketmaster API Configuration
EXPO_PUBLIC_TICKETMASTER_API_KEY=your_ticketmaster_api_key_here
EXPO_PUBLIC_API_BASE_URL=https://app.ticketmaster.com/discovery/v2

# Optional: Enable API request logging (true/false)
EXPO_PUBLIC_ENABLE_API_LOGGING=true

# Optional: Request timeout in milliseconds
EXPO_PUBLIC_API_TIMEOUT=10000
